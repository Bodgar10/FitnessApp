<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.appfitnessapp.app.fitnessapp">

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application

        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Login.SplashPantalla">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Login.IniciarSesion"/>
        <activity android:name=".Login.Registro"
            android:parentActivityName=".Login.IniciarSesion" />

        <activity android:name=".Usuario.DetallePdf"></activity>
        <activity android:name=".Usuario.UsuarioHome"></activity>
        <activity android:name=".Usuario.UsuarioPlan"></activity>
        <activity android:name=".Usuario.UsuarioPerfil"></activity>
        <activity android:name=".Usuario.UsuarioChat"></activity>
        <activity android:name=".Usuario.PantallaPDF"></activity>
        <activity android:name=".Usuario.Imagen"></activity>
        <activity android:name=".Usuario.Video"></activity>
        <activity android:name=".Usuario.MetodoPago"></activity>
        <activity android:name=".Usuario.DatosUsuario"></activity>
        <activity android:name=".Usuario.Tarjeta"></activity>
        <activity android:name=".Usuario.FeedSinRegistro.HomeSinRegistro"></activity>
        <activity android:name=".Usuario.Asesoria"></activity>




    </application>

</manifest>