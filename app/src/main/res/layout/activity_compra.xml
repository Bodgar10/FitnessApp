<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="match_parent"
    tools:context=".Compra.Compra">


    <Button
        android:id="@+id/btn_three_buy_health"
        android:text="Precio 10"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <Button
        android:id="@+id/btn_ten_buy_health"
        android:text="Precio 20"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <Button
        android:id="@+id/btn_twenty_buy_health"
        android:text="Precio 30"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <Button
        android:id="@+id/btn_fifty_buy_health"
        android:text="Precio 40"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />


</LinearLayout>